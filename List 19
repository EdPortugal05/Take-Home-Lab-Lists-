using System;

using System.Linq;

using System.Collections.Generic;



namespace HelloWold;



public static class Program
{
    public static void Main()
    {

        List<string> attendees = new List<string>();

        Console.WriteLine("Enter the names of students logging in (type 'done' to finish):");
        while (true)
        {
            string name = Console.ReadLine();
            if (name.ToLower() == "done") break;

            if (!attendees.Contains(name))
            {
                attendees.Add(name);
            }
        }

        attendees.Sort();

        Console.WriteLine($"\nTotal unique attendees: {attendees.Count}");
        Console.WriteLine("Attendees in alphabetical order:");
        foreach (string attendee in attendees)
        {
            Console.WriteLine(attendee);
        }
    }
}
